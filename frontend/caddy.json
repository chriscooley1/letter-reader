{
  "apps": {
    "http": {
      "servers": {
        "example": {
          "listen": [":8080"],
          "routes": [
            {
              "match": [
                {
                  "path": ["/*"]
                }
              ],
              "handle": [
                {
                  "handler": "rewrite",
                  "uri": "/index.html"
                },
                {
                  "handler": "file_server",
                  "root": "./dist"
                }
              ]
            }
          ]
        }
      }
    }
  }
}
